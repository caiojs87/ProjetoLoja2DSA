@model ProjetoLoja2DSA.Models.Usuario;

@{
    ViewData["Tilte"] = "Cadastro";
}
<form asp-action= "Cadastro" method="post">

    <div>
        <label>Email</label>
        <input type="email" name = "email" required/>
    </div>

    <div>
        <label>Senha</label>
        <input type="password"name="senha" required/>
    </div>

    <button>Cadastro</button>

</form>
